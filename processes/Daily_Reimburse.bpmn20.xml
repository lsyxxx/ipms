      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="595.0" y="207.0"></omgdi:waypoint>
        <omgdi:waypoint x="640.0" y="207.0"></omgdi:waypoint>
