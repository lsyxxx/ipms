        <omgdi:waypoint x="340.0" y="207.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="445.0" y="207.0"></omgdi:waypoint>
        <omgdi:waypoint x="490.0" y="207.0"></omgdi:waypoint>
